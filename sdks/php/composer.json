{
    "name": "dropbox/sign",
    "description": "Dropbox Sign v3 API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "hellosign",
        "dropboxsign"
    ],
    "homepage": "https://hellosign.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Dropbox Sign",
            "homepage": "https://hellosign.com",
            "email": "apisupport@hellosign.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "symfony/yaml": "^5.4"
    },
    "autoload": {
        "psr-4": { "Dropbox\\Sign\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Dropbox\\Sign\\Test\\" : "test/" }
    }
}
